package main
import "fmt"

func main(){
  x := []int{38, 76, 65, 48, 52, 28, 36, 60, 33, 39, 9, 19, 28, 84, 19, 26}

  arrLen := 15
  max := x[0]
  min := x[0]

  for i := 0; i < arrLen; i++{
    if(max > x[i]){
      max = max
    } else{
      max = x[i]
    }
  }

  for j := 0; j < arrLen; j++{
    if(min < x[j]){
      min = min
    } else{
      min = x[j]
    }
  }
  fmt.Println("max value: ",max)
  fmt.Println("min value: ",min)
}
